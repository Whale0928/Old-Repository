package edu.kh.community.board.model.vo;

import lombok.Getter;
import lombok.NoArgsConstructor;
import lombok.Setter;
import lombok.ToString;

@Getter
@Setter
@NoArgsConstructor
@ToString
public class BoardDetail {
	
	private int boardNo;
	private String boardTitle;
	private String boardContent;
	private String createDate;
	private String updateDate;
	private int readCount;
	private String memberNickName;
	private int  profileImg;
	private int memberNo;
	private String boardName;
	
	public BoardDetail(int boardNo, String boardTitle, String boardContent, String createDate, String updateDate,
			int readCount, String memberNickName, int profileImg, int memberNo, String boardName) {
		super();
		this.boardNo = boardNo;
		this.boardTitle = boardTitle;
		this.boardContent = boardContent;
		this.createDate = createDate;
		this.updateDate = updateDate;
		this.readCount = readCount;
		this.memberNickName = memberNickName;
		this.profileImg = profileImg;
		this.memberNo = memberNo;
		this.boardName = boardName;
	}
	
	
	
}
